import java.util.Scanner;

public class _2231 {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int min, max = 0;
		int tmp1, tmp2, tmp3 = 0;
		boolean check = true;
		int r; // 입력값에서 제일 끝 나머지를 뺀 변수
		if (n < 1 || n > 1000000)
			return;
		r = n - (n%10);
		if(r == 0) // 입력값이 10의 자리 이하일때
			r = n;
		while(check) {
			tmp1 = r;
			tmp2 = 0;
			while(true) {
				tmp2 += (tmp1 % 10);
				tmp1 /= 10;
				if(tmp1 == 0)
					break;
			}
			tmp3 = r + tmp2;
			check = false;
		}
		System.out.println(max);
	}
}
