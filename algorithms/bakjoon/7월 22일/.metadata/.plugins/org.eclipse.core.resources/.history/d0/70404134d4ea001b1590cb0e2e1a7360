
public class Example {
	public static void main(String args[]) {
		Gachon gachon = new Gachon();
		Gachon gachon2 = new Gachon(201735944, "컴공", "김형준");
		
		System.out.println(gachon.name);
		System.out.println(gachon2.name);
	}
}

class Gachon{
	int stu_num;
	String major;
	String name;
	Gachon(){
		stu_num = 0;
		major = null;
		name = null;
	}
	Gachon(int stu_num, String major, String name){
		this.stu_num = stu_num;
		this.major = major;
		this.name = name;
	}
}
