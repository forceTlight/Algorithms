import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Solution {
	static boolean check;
	public static void main(String args[]) throws IOException {
		BufferedReader bf = new BufferedReader(new InputStreamReader(System.in));
		int c = Integer.parseInt(bf.readLine());
		int a = 0;
		while (a < c) {
			a++;
			StringTokenizer st = new StringTokenizer(bf.readLine());
			int arr[][] = new int[9][9];
			for (int i = 0; i < 9; i++) {
				for (int j = 0; j < 9; j++) {
					arr[i][j] = Integer.parseInt(st.nextToken());
				}
			}

			// check
			// 가로 체크
			check_width(arr, 0, 0, 1);
			System.out.println(check);
		}
	}
	public static void check_width(int arr[][], int idx, int idx1, int idx2) {
		if(idx == arr.length-1 && idx1 == arr.length-1 && idx2 == arr.length-1)
			return;
		if(idx1 == arr.length-1 && idx2 == arr.length-1) {
			check_width(arr, idx+1, 0, 1);
			return;
		}
		if(idx2 == arr.length-1) {
			check_width(arr, idx, idx1+1, idx1+2);
			return;
		}
		if(arr[idx][idx1] == arr[idx][idx2]) {
			check = false;
		}
		check_width(arr, idx, idx1, idx2+1);
	}
}
