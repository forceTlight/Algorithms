import java.util.Scanner;

public class _2231 {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int min, max = 0;
		int tmp;
		int tmp1 = 0;
		int answer = 0; // 값 저장
		int sum = 0; // 각 자리수 계산
		boolean check = true;
		int r; // 입력값에서 제일 끝 나머지를 뺀 변수
		if (n < 1 || n > 1000000)
			return;
		r = n - (n%10);
//		if(r == 0) // 입력값이 10의 자리 이하일때
//			r = n;
		while(check) {
			tmp1 = r;
			while(tmp1 != 0) {
				sum += (tmp1%10);
				tmp1 /= 10;
			}
			min = r + sum;
			max = r + + sum + (2*9);
			if(n >= min || max <= n) {
				for(int i = 0; i < 9; i++) {
					tmp = r + sum + (2*i);
					if(tmp == n) {
						answer = r + i;
						check = false;
					}
				}
			}
		}
		System.out.println(answer);
	}
}
