import java.util.Scanner;

public class _2231 {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int min, max = 0;
		int tmp;
		int tmp1, tmp2 = 0;
		int answer = 0; // 값 저장
		int sum = 0; // 각 자리수 계산
		boolean check = true;
		int r; // 입력값에서 제일 끝 나머지를 뺀 변수
		if (n < 1 || n > 1000000)
			return;
		r = n - (n % 10);
		int j = 0;
		while (check) {
			tmp2 = r - j;
			tmp1 = tmp2;
			sum = 0;
			while (tmp1 != 0) {
				sum += (tmp1 % 10);
				tmp1 /= 10;
			}
			min = tmp2 + sum;
			max = tmp2 + sum + (2 * 9);
			if (n >= min && n <= max) {
				for (int i = 0; i <= 9; i++) {
					tmp = tmp2 + sum + (2 * i);
					if (tmp == n) {
						answer = tmp2 + i;
					}
				}
				j = j + 10;
			} else {
				check = false;
			}
		}
		if(answer <= 0)
			answer = 0;
		System.out.println(answer);
	}
}
