import java.util.Scanner;

public class _2231 {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int min, max;
		int tmp1, tmp2 = 0;
		boolean check = true;
		int r; // 입력값에서 제일 끝 나머지를 뺀 변수
		if (n < 1 || n > 1000000)
			return;
		r = n - (n%10);
		while(check) {
			tmp1 = r;
			tmp2 = 0;
			while(true) {
				tmp1 /= 10;
				if(tmp1 == 0)
					break;
				tmp2 += (tmp1 % 10);
			}
			max = n + tmp2;
			check = false;
		}
		System.out.println(tmp2);
	}
}
